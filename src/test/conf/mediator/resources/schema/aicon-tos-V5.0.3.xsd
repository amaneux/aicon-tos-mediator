	<xs:element name="aicon-tos-canary-request">
		<xs:annotation>
			<xs:documentation>Watchdog request send every n seconds from Aicon to TOS</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="requestId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Unique id</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="sequence" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>sequential number starting at 1 for the 1st canary message sent.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="requestSendTs" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>UTC timestamp when request was sent by Aicon in ISO8601 format</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="requestSendTsMillis" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>Same timestamp in millis since epoch</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="cdcTables" type="xs:string">
				<xs:annotation>
					<xs:documentation>CSV list of table names to get last generated gkey+creation time for</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-tos-canary-response">
		<xs:annotation>
			<xs:documentation>Canary response from TOS</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="requestId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>echoed from the request.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="sequence" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>echoed from the request</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="requestSendTs" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>timestamp (UTC in ISO8601 format) when request was sent by AICON</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="requestSendTsMillis" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>same but now in millis since epoch</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="requestReceivedTs" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>timestamp (UTC in IO8601 format) when request was received by the TOS</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="requestReceivedTsMillis" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>same but now in millis since epoch</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="responseSendTs" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>timestamp (UTC in ISO8601 format) just before canary is saved and response is sent back  </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="responseSendTsMillis" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>same but now in millis since epoch</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="responseReceivedTs" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>timestamp (UTC in ISO8601 format) when response is received by AICON</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="responseReceivedTsMillis" type="xs:long">
				<xs:annotation>
					<xs:documentation>same but now in millis since epoch</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="latestCdcCreations">
				<xs:annotation>
					<xs:documentation>CSV list of latest record creations of selected tables in the format {[table]=[gkey]:[created_millis][,,....]}</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-tos-control-request">
		<xs:annotation>
			<xs:documentation>Request send to TOS to change settings or perform actions</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="propertyGroup">
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Navis: supports max 6</xs:documentation>
							</xs:annotation>
							<xs:element name="property">
								<xs:complexType>
									<xs:attribute name="key" use="required">
										<xs:annotation>
											<xs:documentation>The key of the property to set</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="value">
										<xs:annotation>
											<xs:documentation>the value of the property to set (when omitted will not be updated but allows to skip to the next property).</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ignore" use="optional">
										<xs:annotation>
											<xs:documentation>the value of the property indicate whether or not to ignore this property.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="groupName" use="required">
							<xs:annotation>
								<xs:documentation>Navis: maps to General Reference.Type</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="requestId" type="xs:string" use="required"/>
			<xs:attribute name="requestTs" type="xs:dateTime" use="required"/>
			<xs:attribute name="ignore" type="xs:boolean" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-tos-control-response">
		<xs:annotation>
			<xs:documentation>Response informing Aicon about result of updates</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="propertyGroup">
					<xs:complexType>
						<xs:sequence minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Navis: supports max 6</xs:documentation>
							</xs:annotation>
							<xs:element name="property">
								<xs:complexType>
									<xs:attribute name="key" use="required">
										<xs:annotation>
											<xs:documentation>The key of the property set</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attributeGroup ref="result"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="groupName" use="required">
							<xs:annotation>
								<xs:documentation>Navis: maps to General Reference.Type</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attributeGroup ref="result"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="requestId" type="xs:string" use="required"/>
			<xs:attribute name="responseTs" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-tos-error-response">
		<xs:annotation>
			<xs:documentation>Response to any error that wouldn't fit in a normal response (e.g. message problem)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attributeGroup ref="result"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-yard-decking-update-request">
		<xs:annotation>
			<xs:documentation>Request from yardsight to deck (change position) of 1 or more moves.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="moves">
					<xs:annotation>
						<xs:documentation>All moves belonging to a single physical handling like twin carry.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="deckMove">
								<xs:complexType>
									<xs:sequence minOccurs="0">
										<xs:element ref="map"/>
									</xs:sequence>
									<xs:attribute name="wiGkey" use="required">
										<xs:annotation>
											<xs:documentation>The gkey of the WorkInstruction</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="positionTo" type="xs:string" use="required">
										<xs:annotation>
											<xs:documentation>new planned position in customer specific format</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="requestId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>id of the request itself, to be echoed back within the response to match it against</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="timeStamp" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>time the request was sent in millis</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="timeStampLocalTimeISO8601" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>time the request was sent in local ISO8601 format incl. ms</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-yard-decking-update-response">
		<xs:annotation>
			<xs:documentation>Response to a yardsight decking request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="moves" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="deckMove" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="wiGkey" use="required"/>
									<xs:attributeGroup ref="result"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="requestId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The requestId from the request this response belongs to.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="timeStampLocalTimeISO8601" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>time the response was sent in local ISO8601 format incl. ms</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="timeStamp" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>time the request was sent in millis</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="result"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-yard-move-request">
		<xs:annotation>
			<xs:documentation>Request from yardsight to execute 1 or more moves</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="workQueue" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence minOccurs="0">
							<xs:element ref="map" minOccurs="0">
								<xs:annotation>
									<xs:documentation>A map of key-value pairs for temporary interface enhancements only</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:choice minOccurs="0" maxOccurs="unbounded">
								<xs:element name="addMove">
									<xs:complexType>
										<xs:sequence minOccurs="0">
											<xs:element ref="map"/>
										</xs:sequence>
										<xs:attribute name="containerId" use="required">
											<xs:annotation>
												<xs:documentation>ISO6346 container identification</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="positionTo" type="xs:string" use="required">
											<xs:annotation>
												<xs:documentation>new planned position in customer specific format</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="positionFrom">
											<xs:annotation>
												<xs:documentation>When given will be matched against tos current pos; expect it in the same format as positionTo</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="moveSequence" type="xs:long">
											<xs:annotation>
												<xs:documentation>defines the order of the moves we have to create in, sequence doesn't need to be received in order of appearance</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="moveType">
											<xs:annotation>
												<xs:documentation>Defines the type of planned move created by the housekeeping module, no direct relation with TOS field.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="HOUSEKEEPING"/>
													<xs:enumeration value="PREPOSITIONING"/>
													<xs:enumeration value="CLEARING"/>
													<xs:enumeration value="REHANDLE"/>
													<xs:enumeration value="INTERMEDIATE"/>
													<xs:enumeration value="INTERIO"/>
													<xs:enumeration value="NONE"/>
													<xs:enumeration value="UNKNOWN"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="moveKind">
											<xs:annotation>
												<xs:documentation>The kind of move for the created WorkInstruction. Defaults to YARD when not given.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="YARD"/>
													<xs:enumeration value="SHFT"/>
													<xs:enumeration value="RECV"/>
													<xs:enumeration value="DLVR"/>
													<xs:enumeration value="DSCH"/>
													<xs:enumeration value="LOAD"/>
													<xs:enumeration value="RDSC"/>
													<xs:enumeration value="RLOD"/>
													<xs:enumeration value="SHOB"/>
													<xs:enumeration value="OTHR"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="targetMoveGkey" type="xs:long">
											<xs:annotation>
												<xs:documentation>The WI gkey of the target move (e.g. a DLVR) which needs 1 or more SHFT's to get uncovered (needed when moveKind=SHFT)</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
								<xs:element name="deleteMove">
									<xs:complexType>
										<xs:sequence minOccurs="0">
											<xs:element ref="map"/>
										</xs:sequence>
										<xs:attribute name="wiGkey" use="required">
											<xs:annotation>
												<xs:documentation>The gkey of the WorkInstruction</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
								<xs:element name="updateMove">
									<xs:complexType>
										<xs:sequence minOccurs="0">
											<xs:element ref="map"/>
										</xs:sequence>
										<xs:attribute name="wiGkey" use="required">
											<xs:annotation>
												<xs:documentation>The gkey of the WorkInstruction</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="positionTo" type="xs:string">
											<xs:annotation>
												<xs:documentation>new planned position in customer specific format</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="priority" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Gives this move a priority when set to true (falso does not remove priority).</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
							</xs:choice>
						</xs:sequence>
						<xs:attribute name="wqName" type="xs:string">
							<xs:annotation>
								<xs:documentation>Name of the work queue</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="wqType" default="YARD">
							<xs:annotation>
								<xs:documentation>To be set when work queue needs to be created</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="DISCH"/>
									<xs:enumeration value="LOAD"/>
									<xs:enumeration value="YARD"/>
									<xs:enumeration value="PROJECTION"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="powName" type="xs:string">
							<xs:annotation>
								<xs:documentation>PointOfWork name, needed when workQueue needs to be created</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="powPushRate" type="xs:long" default="30">
							<xs:annotation>
								<xs:documentation>in moves/hour</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="chePoolName" type="xs:string">
							<xs:annotation>
								<xs:documentation>Che Pool name, needed when POW needs to be created</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="shiftName" type="xs:string">
							<xs:annotation>
								<xs:documentation>Crane shift name, needed when POW needs to be created</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="allowCreation" type="xs:boolean" default="true">
							<xs:annotation>
								<xs:documentation>when true allows workQueue creation</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="allowActivation" type="xs:boolean" default="true">
							<xs:annotation>
								<xs:documentation>when true allows workQueue activation</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="requestId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>id of the request itself, to be echoed back within the response to match it against</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="timeStamp" type="xs:long" use="required">
				<xs:annotation>
					<xs:documentation>time the request was sent in millis</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="timeStampLocalTimeISO8601" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>time the request was sent in local ISO8601 format incl. ms</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="jobRequestId" type="xs:string">
				<xs:annotation>
					<xs:documentation>The id of the housekeeping job</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="moveRequestNumber" type="xs:long">
				<xs:annotation>
					<xs:documentation>Sequence of moveRequest within the same job.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="aicon-yard-move-response">
		<xs:annotation>
			<xs:documentation>Response to a yardsight move request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="workQueue">
					<xs:complexType>
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element name="addMove">
								<xs:complexType>
									<xs:attributeGroup ref="result"/>
									<xs:attribute name="containerId" use="required">
										<xs:annotation>
											<xs:documentation>The container isoId</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="wiGkey">
										<xs:annotation>
											<xs:documentation>the gkey of the created WorkInstruction, to be used in the wiGkey when it needs to be deleted or updated.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="wiSequence" type="xs:long">
										<xs:annotation>
											<xs:documentation>will be the sequence of the WI created (can be different from the moveSequence)</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="deleteMove">
								<xs:complexType>
									<xs:attributeGroup ref="result"/>
									<xs:attribute name="wiGkey" use="required"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="updateMove">
								<xs:complexType>
									<xs:attributeGroup ref="result"/>
									<xs:attribute name="wiGkey" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:choice>
						<xs:attribute name="wqName" use="required">
							<xs:annotation>
								<xs:documentation>The name of the workQueue</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attributeGroup ref="result"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="requestId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The requestId from the request this response belongs to.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="timeStampLocalTimeISO8601" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>time the response was sent in local ISO8601 format incl. ms</xs:documentation>
				</xs:annotation>
